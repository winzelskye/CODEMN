    attachedObjectDelay: 1
    hideOnNextMessage: 1
  - characterName: Handler
    dialogueText: HTML or Hyper Text Markup Language is the standard markup language
      for the creation Web pages.
    textColor: {r: 1, g: 0.92156863, b: 0.015686275, a: 1}
    isPlayer: 0
    messageSound: {fileID: 8300000, guid: 2310fdff703bd2c4db3c4c2930812960, type: 3}
